{
    "extends": ["airbnb","prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"]
    },
    "env": {
        "node": true,
        "es6": true
        
    },
    "parser": "babel-eslint",
    "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
    "globals": {
        "before": true,
        "after": true,
        "describe": true,
        "beforeEach": true,
        "it": true,
        "expect": true,
        "document": true,
        "test": true,
        "jest":true,
        "afterEach": true

    }
}


